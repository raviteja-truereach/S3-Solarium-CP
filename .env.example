# Azure Application Insights Configuration
# These values should be injected by CI/CD from Azure Key Vault
AZURE_APPLICATION_INSIGHTS_KEY=your-instrumentation-key-here
ENABLE_TELEMETRY=false
TELEMETRY_BATCH_SIZE=10
TELEMETRY_FLUSH_INTERVAL=30000
TELEMETRY_MAX_RETRIES=3
TELEMETRY_RETRY_DELAY=1000
ENABLE_USER_TRACKING=false
ENABLE_PERFORMANCE_TRACKING=true
ENABLE_MEMORY_TRACKING=true
ENABLE_DOCUMENT_UPLOAD=true
# Logging configuration  
ENABLE_DEBUG_LOGS=false
ENABLE_PERFORMANCE_LOGS=false